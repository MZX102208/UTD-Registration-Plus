{
  "name": "UTD Registration Plus",
  "version": "1.0",
  "description": "Improves the course registration process at the University of Texas at Dallas!",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "content_scripts": [{
    "css": ["css/styles.css"],
    "js": ["lib/jquery-3.3.1.min.js", "lib/jquery.initialize.min.js", "js/util.js", "js/content.js"],
    "matches": ["https://coursebook.utdallas.edu/*"]
  }],
  "web_accessible_resources": [
    "images/*",
    "html/*",
    "css/*",
    "lib/*",
    "js/registration-modal.js"
  ],
  "manifest_version": 2
}

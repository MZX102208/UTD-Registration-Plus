{
  "name": "UTD Registration Plus",
  "version": "1.0",
  "description": "Improves the course registration process at the University of Texas at Dallas!",
  "permissions": ["tabs",
    "declarativeContent",
    "storage",
    "*://coursebook.utdallas.edu/*"],
  "options_page": "options.html",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "content_scripts": [{
    "css": ["css/styles.css"],
    "js": ["lib/jquery-3.4.1.min.js", "lib/jquery.initialize.min.js", "lib/highcharts.js",
           "js/util.js", "js/content.js", "js/registration-modal.js",
           "objects/schedule-block.js", "objects/course.js"],
    "matches": ["https://coursebook.utdallas.edu/*"]
  }],
  "web_accessible_resources": [
    "images/*",
    "html/*",
    "css/*"
  ],
  "manifest_version": 2
}
